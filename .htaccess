RewriteEngine On
RewriteRule ^welcome$ welcome.php [L]
RewriteRule ^welcome/$ welcome.php [L]
RewriteRule ^job/new newJob.php
RewriteRule ^job/([0-9,a-z,A-Z]+)$ newJob.php?id=$1 [L]
RewriteRule ^jobs/$ joblist.php
RewriteRule ^jobs$ joblist.php [L]
RewriteRule ^equipment/([0-9,a-z,A-Z]+)$ equipment.php?id=$1
RewriteRule ^equipment/([0-9,a-z,A-Z]+)/$ equipment.php?id=$1
RewriteRule ^equipment/$ equipmentlist.php
RewriteRule ^equipment$ equipmentlist.php
RewriteRule ^equipmentlist/$ equipmentlist.php
RewriteRule ^equipmentlist$ equipmentlist.php [L]
RewriteRule ^employee/([0-9,a-z,A-Z]+)$ employees.php?id=$1
RewriteRule ^employee/([0-9,a-z,A-Z]+)/$ employees.php?id=$1
RewriteRule ^employees/$ employeelist.php
RewriteRule ^employees$ employeelist.php [L]
